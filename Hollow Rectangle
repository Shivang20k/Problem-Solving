int main()
{
	// Your code goes here;
	int r, c;
	cin >> r;
	cin >> c;
  for(int row = 0; row < r; row++)
	{
		if(row ==0 || row==r-1)
		for(int col = 0; col < c ; col ++)
		   cout << "X  ";
		else
		{
			cout << "X ";
			for(int col =1 ; col < c-1 ; c++)
			  cout << "  ";
		    cout << "X ";
		}
		cout << endl;
	}
